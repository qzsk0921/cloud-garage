/**index.wxss**/
.container-home {
  background-color: #fff;
}

.fixed {
  top: 0;
  position: fixed;
  width: 100%;
  z-index: 1;
}

/* .scroll-box {
  padding-top: 158rpx;
} */
.section1 {
  padding-top: 20rpx;
  position: relative;
  height: 72rpx;
  z-index: 3;
  background: #FFFFFF;
}

.input-box {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.btn-dropdown {
  padding: 0 35rpx 0 28rpx;
  width: 148rpx;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 30rpx;
  font-weight: 500;
  color: #333333;
  position: relative;
}

.btn-dropdown:after {
  /* margin-left: 12rpx; */
  position:absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-20%);
  display: inline-block;
  content: '';
  width: 0;
  height: 0;
  border-top: 10rpx solid #333333;
  border-bottom: 10rpx solid transparent;
  border-left: 7rpx solid transparent;
  border-right: 7rpx solid transparent;
}

.search {
  height: 100%;
  width: 570rpx;
  background: #FFFFFF;
  border: 2rpx solid #9FBAF9;
  border-radius: 8rpx;
  padding-left: 70rpx;
  position: relative;
}

.search::before,
.search::after {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  content: '';
  background-size: 100% 100%;
}

.search::before {
  left: 24rpx;
  width: 31rpx;
  height: 30rpx;
  background-image: url("data:image/png;base64,");
}

.search::after {
  width: 34rpx;
  height: 34rpx;
  right: 18rpx;
  background-image: url("data:image/png;base64,");
}

.search-placeholder {
  font-size: 28rpx;
  /* line-height: 100%; */
  color: var(--primary-color);
}

.section2.condition-box {
  position: relative;
  display: flex;
  z-index: 2;
}

.section2 .condition {
  width: 25%;
}

.section2 .condition-text {
  text-align: center;
  padding-top: 20rpx;
  height: 86rpx;
  line-height: 66rpx;
  position: relative;
  z-index: 1;
  background: #FFFFFF;
  font-size: 26rpx;
  color: #333333;
}

.section2 .condition-text.active-color {
  font-weight: bold;
}

.section2 .condition .condition-text::after {
  display: inline-block;
  width: 0;
  height: 0;
  content: '';
  border-top: 6rpx solid #333;
  border-bottom: 6rpx solid transparent;
  border-left: 6rpx solid transparent;
  border-right: 6rpx solid transparent;
  top: 47rpx;
  z-index: 1;
  margin-left: 12rpx;
  transition: transform .2s;
}

.section2 .condition .condition-text.active-rotate::after {
  transform: rotate(180deg);
  transform-origin: 6rpx 3rpx;
}

.section3 {
  height: 112rpx;
  padding: 24rpx 0;
  background: #F8F9FC;
  overflow: scroll;
  transition: all .4s;
}

.section3.clear {
  visibility: hidden;
  opacity: 0;
  height: 0;
}

.section3 .condition-box {
  display: flex;
  border: 1px solid transparent;
  width: max-content;
  padding-right: 175rpx;
}

.section3 .condition-box .condition-tag:first-child {
  margin-left: 40rpx;
}

.section3 .condition-box .condition-tag:not(:last-child) {
  margin-right: 22rpx;
}

.section3 .condition-box .condition-tag {
  line-height: 64rpx;
  padding: 0 20rpx;
  height: 64rpx;
  background: #FFFFFF;
  border-radius: 4rpx;
}

.section3 .condition-box .condition-tag .close-tap {
  margin-left: 15rpx;
}

.btn-reset {
  text-align: center;
  /* height: 112rpx; */
  position: absolute;
  top: 0;
  right: 0;
  line-height: 112rpx;
  width: 128rpx;
  background: #F8F9FC;
  font-size: 24rpx;
  color: #787C8C;
}

.btn-reset::before {
  pointer-events: none;
  position: absolute;
  right: 100%;
  content: '';
  width: 47rpx;
  height: 113rpx;
  content: '';
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.section4 {}

.scroll-box {
  /* height: calc(100vh - 124px); */
  overflow: auto;
}

.scroll-box-sharer {
