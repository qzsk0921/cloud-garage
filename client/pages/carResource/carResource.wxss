.container-resource .activity-list .card .touchable .touchable-right .edit,
.container-resource .activity-list .card .touchable .touchable-right .delete {
  width: 98rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  text-align: center;
  display: inline-block;
}

.container-resource .activity-list .card .touchable .touchable-right .edit {
  background: #FF9E3D;
}

.container-resource .activity-list .card .touchable .touchable-right .delete {
  background: #F53D3D;
}

.container-resource .activity-list .card:not(:first-child) {
  margin-top: 20rpx;
}

.container-resource .activity-list .card .c-top {
  padding: 24rpx 0 0 30rpx;
  display: flex;
  justify-content: space-between;
}

.container-resource .activity-list .card .c-top .c-t-left .icon {
  width: 30rpx;
  height: 30rpx;
  margin-right: 12rpx;
  vertical-align: top;
}

.container-resource .activity-list .card .c-top .c-t-left .tit {
  font-size: 24rpx;
  font-weight: 500;
  color: #333333;
}

.container-resource .activity-list .card .c-top .c-t-right .desc {
  font-size: 24rpx;
}

.container-resource .activity-list .card .c-top .c-t-right .icon {
  width: 20rpx;
  height: 16rpx;
  margin-left: 10rpx;
  display: inline-block;
}

.container-resource .activity-list .card .c-top .c-t-right.status1 .desc {
  color: #7F93D6;
}

.container-resource .activity-list .card .c-top .c-t-right.status1 .icon {
  background: #7F93D6;
}

.container-resource .activity-list .card .c-top .c-t-right.status2 .desc {
  color: #F55858;
}

.container-resource .activity-list .card .c-top .c-t-right.status2 .icon {
  background: #F55858;
}

.container-resource .activity-list .card .c-top .c-t-right.status3 .desc {
  color: #00AC06;

}

.container-resource .activity-list .card .c-top .c-t-right.status3 .icon {
  background: #00AC06;
}

.container-resource .activity-list .card .c-top .c-t-right.status4 .desc {
  color: #888888;

}

.container-resource .activity-list .card .c-top .c-t-right.status4 .icon {
  background: #888888;
}

.container-resource .activity-list .card .c-center {
  padding: 20rpx 40rpx 40rpx;
  display: flex;
  border-bottom: 2rpx solid #EEEEEE;
  position: relative;
}

.container-resource .activity-list .card .c-center .tip {
  position: absolute;
  top: 20rpx;
  left: 40rpx;
  width: 76rpx;
  height: 48rpx;
  border-radius: 8rpx 0rpx 8rpx 0rpx;
  font-size: 24rpx;
  font-weight: 500;
  line-height: 48rpx;
  color: #FFFFFF;
  text-align: center;
}

.container-resource .activity-list .card .c-center .c-c-left {
  width: 254rpx;
  height: 190rpx;
  border-radius: 8rpx;
  margin-right: 30rpx;
}

.container-resource .activity-list .card .c-center .c-c-right {
  width: calc(100% - 284rpx);
  position: relative;
}

.container-resource .activity-list .card .c-center .c-c-right .l1 {
  font-size: 30rpx;
  font-weight: 500;
  line-height: 40rpx;
  color: #333333;
  /* height: 72rpx; */
}

.container-resource .activity-list .card .c-center .c-c-right .l2 {
  margin-top: 16rpx;
}

.container-resource .activity-list .card .c-center .c-c-right .l2 view {
  font-size: 22rpx;
  color: #AAAAAA;
  display: inline-block;
}

.container-resource .activity-list .card .c-center .c-c-right .l2 view:not(:first-child) {
  padding-left: 10rpx;
  border-left: 1rpx solid #AAAAAA;
}

.container-resource .activity-list .card .c-center .c-c-right .l2 view:not(:last-child) {
  padding-right: 10rpx;
}

.container-resource .activity-list .card .c-center .c-c-right .l3 {
  position: absolute;
  bottom: 10rpx;
  /* margin-top: 30rpx; */
}

.container-resource .activity-list .card .c-center .c-c-right .l3 view {
  display: inline-block;
  font-size: 32rpx;
  color: #EC5846;
}

.container-resource .activity-list .card .c-center .c-c-right .l3 view text:nth-of-type(1) {
  font-size: 32rpx;
  font-weight: bold;
  color: #EC5846;
}

.container-resource .activity-list .card .c-center .c-c-right .l3>text {
  margin-left: 20rpx;
  font-size: 22rpx;
  color: #AAAAAA;
}

.container-resource .activity-list .card .c-center .c-c-right .l4 {
  margin-top: 18rpx;
  font-size: 22rpx;
  color: #999999;
}

.container-resource .activity-list .card .c-bottom {}

.container-resource .activity-list .card .c-bottom .btn {
  display: inline-block;
  width: 50%;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
}

.container-resource .activity-list .card .c-bottom .btn image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
}

.container-resource .activity-list .card .c-bottom .btn text {
  font-size: 26rpx;
  font-weight: 500;
  color: #444444;
}

