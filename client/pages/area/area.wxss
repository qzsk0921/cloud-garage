.container-area {
  background: #fff;
}

.input-box {
  padding: 20rpx 32rpx 40rpx;
  display: flex;
  align-items: center;
}

.input-box .search {
  width: 594rpx;
  height: 72rpx;
  background: #F2F4F9;
  border-radius: 8rpx;
  padding-left: 76rpx;
  position: relative;
}

.input-box .search:before {
  content: '';
  width: 24rpx;
  height: 25rpx;
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.input-box .search-placeholder {
  font-size: 30rpx;
  color: #969AAA;
}

.input-box .btn-search {
  font-size: 30rpx;
  font-weight: 500;
  color: #333333;
  margin-left: 32rpx;
}

.container-inner {
  display: flex;
  flex-direction: row-reverse;
}

.list {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
}

.searchLetter {
  flex-shrink: 0;
  width: 80rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
  color: #666;
}

.searchLetter view {
  margin-top: 20rpx;
}

.touchClass {
  background-color: #fff;
  color: #fff;
  padding-top: 16rpx;
  padding-bottom: 16rpx;
}

.showSlectedLetter {
  background-color: rgba(0, 0, 0, 0.5);
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 50%;
  left: 50%;
  margin: -100rpx;
  width: 200rpx;
  height: 200rpx;
  border-radius: 20rpx;
  font-size: 52rpx;
  z-index: 1;
}

.selection {
  display: flex;
  width: 100%;
  flex-direction: column;
}

.selectCity {
  padding: 10rpx 0 0 35rpx;
  /* background-color: #f5f5f5; */
  /* margin-bottom: -10rpx; */
}

.item_letter {
  display: flex;
  background: #F5F6FA;
  height: 44rpx;
  padding-left: 40rpx;
  align-items: center;
  font-size: 26rpx;
  color: #666;
}

.item_city {
  display: flex;
  height: 100rpx;
  padding-left: 40rpx;
  align-items: center;
  border-bottom: 1rpx solid #E8E8E8;
  font-size: 30rpx;
  color: #666;
}

.item_city:last-child {
  border-bottom: none;
}

.hotcity-common {
  font-size: 24rpx;
  color: #666;
  padding-bottom: 0;
  margin: 8rpx 0;
  margin-left: 16rpx;
}

.hotCity {
  padding-right: 50rpx;
  margin: auto;
}

.thisCityName {
  display: inline-block;
  border: 1rpx solid #FEC240;
  border-radius: 8rpx;
  padding: 10rpx 0;
  font-size: 24rpx;
  color: #FEC240;
  text-align: center;
  min-width: 149.5rpx;
  margin: 16rpx 0;
}

.thishotText {
  font-size: 20rpx;
  color: #333333;
}

.letter {
  font-size: 20rpx;
  /* line-height: 34rpx; */
  color: #333333;
}

.slectCity {
  border-color: #FEC240 !important;
}

.slectCity view {
  color: #FEC240 !important;
}

.weui-grid {
  padding: 10rpx 0;
  width: 200rpx;
  box-sizing: border-box;
  border: 1rpx solid #ececec;
  border-radius: 8rpx;
  background-color: white;
  margin: 8rpx 0;
}

.weui-grids {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.weui-grid__label {
  display: block;
  text-align: center;
  color: #333;
  font-size: 24rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.ul {
  display: block;
  color: grey;
  margin-left: 20rpx;
}

.li {
  display: block;
  font-weight: 100;
  font-size: 28rpx;
  padding: 16rpx 0;
}

.county {
  display: flex;
  flex-wrap: wrap;
}