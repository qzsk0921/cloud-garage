.footer {
  text-align: center;
  position: fixed;
  bottom: 20rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 686rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: var(--primary-color);
  border-radius: 8rpx;
  font-size: 30rpx;
  color: #FFFFFF;
}

.side-toolbar {
  position: fixed;
  bottom: 60rpx;
  right: 40rpx;
  width: 100rpx;
  height: 100rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 6rpx 24rpx rgba(51, 51, 51, 0.16);
  border-radius: 50%;
  line-height: 100rpx;
  text-align: center;
}

.side-toolbar .icon-more {
  width: 70rpx;
  height: 56rpx;
}