/* 价格 */
.tree-select__item .price .item-btn.active-btn {
  background: rgba(74, 116, 246, 0.12);
  border-color: var(--primary-color);
  color: var(--primary-color);
}

.tree-select__item .price .section {}

.tree-select__item .price .section .tit {
  margin-top: 24rpx;
  text-align: left;
  line-height: 1;
}

.tree-select__item .price .section .tit text:nth-of-type(1) {
  font-size: 24rpx;
  color: #333333;
}

.tree-select__item .price .section .tit text:nth-of-type(2) {
  font-size: 24rpx;
  color: #999999
}

.tree-select__item .price .section .customprice-box {
  margin-top: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.tree-select__item .price .section .input-price {
  text-align: center;
  width: 228rpx;
  height: 74rpx;
  background: #F7F8FC;
  border-radius: 8rpx;
}

.tree-select__item .price .section .input-placeholder {
  font-size: 24rpx;
  color: #B5B8C4;
}

.input-placeholder {
  font-size: 24rpx;
  color: #B5B8C4;
}

.tree-select__item .price .section .strikethrough {
  margin: 0 12rpx;
  width: 30rpx;
  height: 3rpx;
  background: #D6D6D6;
}

/* 滑动样式 */
.select-construct {
  width: 100%;
}

.select-area {
  width: 100%;
  padding: 10rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.slider-container {
  width: 100%;
  display: flex;
  padding: 20px 0 0;
  position: relative;
}

.slider-left,
.slider-right {
  margin-right: -8rpx;
}

.footer {
  height: 120rpx;
  width: 100vw;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #FFFFFF;
  z-index: 1;
}

.footer .btn {
  width: 188rpx;
  height: 80rpx;
  line-height: 80rpx;
  border: 1rpx solid #BEC5CF;
  text-align: center;
}

.footer .btn-reset {
  width: 188rpx;
  border-radius: 10rpx;
  color: #5F6670;
  display: flex;
  justify-content: center;
  align-items: center;
}

.footer .btn-reset image {
  margin-right: 10rpx;
  width: 38rpx;
  height: 38rpx;
}

.footer .btn-submit {
  width: 474rpx;
  background: #4A74F6;
  border-radius: 8rpx;
  color: #FFFFFF;
}

.footer .btn-reset {
  margin-right: 24rpx;
}