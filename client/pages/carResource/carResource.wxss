/* pages/carResource/carResource.wxss */
.container-resource {
  background: #ffffff;
}

.fixed {
  top: 0;
  position: fixed;
  width: 100%;
  z-index: 10000;
}

.section1 {
  padding-top: 20rpx;
  position: relative;
  z-index: 3;
  background: #FFFFFF;
}

.input-box {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-left: 32rpx;
}

.search {
  height: 72rpx;
  width: 594rpx;
  background: #F2F4F9;
  border-radius: 8rpx;
  border-radius: 8rpx;
  padding-left: 76rpx;
  position: relative;
}

.search::before {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  content: '';
  background-size: 100% 100%;
}

.search::before {
  left: 30rpx;
  width: 31rpx;
  height: 30rpx;
  background-image: url("data:image/png;base64,");
}

.search-placeholder {
  font-size: 30rpx;
  color: #969AAA;
}

.btn-search {
  text-align: center;
  width: 124rpx;
  font-size: 30rpx;
  font-weight: 500;
  line-height: 72rpx;
  color: #333333;
}

.scroll-box {
  /* height: calc(100vh - 124px); */
  overflow: auto;
}

.scroll-box-sharer {
  margin-top: 96rpx;
}





.section3 {
  /* display: flex;
  justify-content: space-around; */
  position: relative;
  background: #ffffff;
  white-space: nowrap;
  overflow: scroll;
}

.section3::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}

.section3 .tab {
  width: 165rpx;
  display: inline-block;
  padding: 44rpx 0 28rpx;
  /* flex: 1; */
  text-align: center;
  font-size: 28rpx;
  position: relative;
  color: #666666;
  font-size: 26rpx;
  font-weight: 500;
}

.section3 .tab.active {
  color: #333333;
}

.section3 .tab>view {
  display: inline-block;
}

.section3 .tab .num {
  margin-top: 8rpx;
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  position: relative;
}

.section3 .tab.active {
  /* color: #333333; */
}

.section3 .tabs-line {
  box-sizing: border-box;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 1;
  /* width: 0;
  height: 0;
  border: 12rpx;
  border-style: solid;
  border-color: transparent transparent #fff; */
  transition-duration: .3s;
  width: 30rpx;
  height: 6rpx;
  background: var(--primary-color);
  border-radius: 10rpx;
}

.section4 {
  background: #F7F8FC;
}

.section4 .no-data {
  background: #ffffff;
}

.no-data image {
  width: 500rpx;
  height: 300rpx;
}

.section4 .no-data image {
  display: block;
  margin: 125rpx auto 0;
  width: 420rpx;
  margin-bottom: 40rpx;
}

.section4 .no-data view {
  text-align: center;
  font-size: 30rpx;
  line-height: 51rpx;
  color: #888888;
}

