<!-- 开通会员 -->
<view class="dialog-wrap {{dialogVisible?'':'fadeout'}}" style="top:{{navHeight}}px">
  <view class="mask" bindtap="dialogTapHandle"></view>
  <view class="dialog">
    <view class="content">
      <view class="row1">
        <image class="icon-tip" src="../../assets/images/my_car_popup_off.png" mode="aspectFill"></image>
      </view>
      <view class="row2">
        <view>非云车会员用户无法编辑发布车源
        </view>
        <view>快去开通云车会员吧~</view>
      </view>
      <view class="row3">
        <view class="btn btn-reg">立即开通</view>
        <view class="btn-close btn" bindtap="closeHandle">残忍拒绝</view>
      </view>
    </view>
  </view>
</view>