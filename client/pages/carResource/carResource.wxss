  /* overflow-x: hidden; */
  /* background: #F9F9F9; */
  font-size: 0;
  /* width: 100vw; */
}

.container-resource .activity-list.sharer {
  /* margin-top: 94rpx; */
}

.container-resource .activity-list .card {
  background: #ffffff;
}

.container-resource .activity-list .card .touchable {
  /* background: #FFFFFF; */
